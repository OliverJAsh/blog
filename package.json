{
  "private": true,
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "prestart": "npm run compile",
    "start": "npm run runServer",
    "compile": "npm run clean && npm run compileClient && cp -r client/target/* server/target/client && npm run compileServerTs && cp -r server/src/posts server/target",
    "clean": "rm -rf server/target && rm -rf client/target && mkdir -p server/target/client && mkdir -p client/target",
    "compileClient": "webpack && cp -r client/src/images client/target && cp ./node_modules/prismjs/themes/prism.css ./client/target/prism-theme.css",
    "compileClientTs": "tsc --project client",
    "compileServerTs": "tsc --project server",
    "runServer": "node server/target/index.js >> log.txt"
  },
  "dependencies": {
    "@types/compression": "^0.0.33",
    "@types/denodeify": "^1.2.30",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.62",
    "@types/node": "^7.0.12",
    "@types/prismjs": "^1.6.1",
    "@types/virtual-dom": "^2.0.27",
    "compression": "^1.5.2",
    "dateformat": "https://registry.npmjs.org/dateformat/-/dateformat-1.0.12.tgz",
    "denodeify": "^1.2.1",
    "express": "^4.13.3",
    "loadcss": "^0.0.2",
    "lodash": "^4.7.0",
    "prismjs": "^1.6.0",
    "slug": "^0.9.1",
    "ts-loader": "^2.0.3",
    "typescript": "2.2.2",
    "vdom-to-html": "^2.1.1",
    "virtual-dom": "^2.1.1",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.1"
  }
}
